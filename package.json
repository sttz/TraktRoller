{
  "name": "tracktroller",
  "version": "1.0.5",
  "description": "Trakt.tv scrobbler for Crunchyroll",
  "main": "source/index.ts",
  "author": "Adrian Stutz",
  "license": "MIT",
  "browserslist": "> 5%",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-buildenv": "./parcel-plugin-buildenv",
    "parcel-plugin-header": "./parcel-plugin-header",
    "prop-types": "^15.7.2",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "dev": "parcel watch --no-hmr source/index.ts",
    "build": "npm run clean && parcel build --out-file TraktRoller.user.js --no-minify source/index.ts"
  },
  "header": "userscript_header.txt",
  "dependencies": {
    "emotion": "^10.0.27",
    "preact": "10.1.1",
    "strongly-typed-events": "^1.6.3"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  }
}
